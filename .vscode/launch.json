{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core NorthwindApi",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildNorthwindApi",
            "program": "${workspaceFolder}/NorthwindApi/bin/Debug/net5.0/NorthwindApi.dll",
            "args": [],
            "cwd": "${workspaceFolder}/NorthwindApi",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
        {
            "name": ".NET Core YoloShop",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildYoloShop",
            "program": "${workspaceFolder}/YoloShop/bin/Debug/net5.0/YoloShop.dll",
            "args": [],
            "cwd": "${workspaceFolder}/YoloShop",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}